<?xml version="1.0" encoding="UTF-8"?>
<Export generator="Cache" version="25">
<CSP name="partials/admin_adduser.html" application="/csp/vacancy/"><![CDATA[

<div ng-include="'partials/tab_menu.html'" ng-init="usr_add_active = 'active'"></div>

<div  class="row">
    <div class="span12">

        <div class="well">
            <form class="form-horizontal"  name="NewSystemUser" ng-model="u"  ng-submit="newuser(u)">


                <div class="control-group">
                    <label for="FirstName" class="control-label" >Имя:</label>
                    <div class="controls"><input type="text" ng-model="u.FirstName"/></div>
                </div>

                <div class="control-group">
                    <label for="LastName" class="control-label" >Фамилия:</label>
                    <div class="controls"><input type="text" ng-model="u.LastName" /></div>
                </div>


                <div class="control-group">
                    <label for="Login" class="control-label" >Логин:</label>
                    <div class="controls"><input type="text" ng-model="u.Login" required="" ng-change="LoginVerify(u.Login)"/>
                        <div class="label label-warning">{{isLogin}}</div>
                    </div>

                </div>

                <div class="control-group">
                    <label for="Password" class="control-label" >Пароль:</label>
                    <div class="controls"><input type="text" ng-model="u.Password" required=""/></div>
                </div>

                <div class="control-group">
                    <label for="Sex" class="control-label" >Пол:</label>
                    <div class="controls"><select name="Sex" ng-model="u.Sex">
                        <option value="М">М</option>
                        <option value="Ж">Ж</option>
                    </select>
                    </div>
                </div>

                <div class="control-group">
                    <label for="UserType" class="control-label" >Тип пользователя:</label>
                    <div class="controls"><input type="text" ng-model="u.UserType"/></div>
                </div>

                <div class="control-group">
                    <label for="DOB" class="control-label" >Дата рождения:</label>
                    <div class="controls"><input type="text" ng-model="u.DOB"/></div>
                </div>

                <div class="control-group">
                    <label for="Country" class="control-label" >Страна:</label>
                    <div class="controls"><input type="text" ng-model="u.Country"/></div>
                </div>

                <div class="control-group">
                    <label for="State" class="control-label" >Край/область:</label>
                    <div class="controls"><input type="text" ng-model="u.State"/></div>
                </div>

                <div class="control-group">
                    <label for="City" class="control-label">Город:</label>
                    <div class="controls"><input type="text" ng-model="u.City"/></div>
                </div>

                <div class="control-group">
                    <label for="Street" class="control-label">Улица:</label>
                    <div class="controls"><input type="text" ng-model="u.Street"/></div>
                </div>

                <div class="control-group">
                    <label for="Building" class="control-label">Строение:</label>
                    <div class="controls"><input type="text" ng-model="u.Building"/></div>
                </div>

                <div class="control-group">
                    <label for="Apartament" class="control-label">Квартира:</label>
                    <div class="controls"><input type="text" ng-model="u.Apartament"/></div>
                </div>

                <div class="control-group">
                    <label for="Email" class="control-label">E-mail:</label>
                    <div class="controls"><input type="text" ng-model="u.Email"/></div>
                </div>

                <div class="control-group">
                    <label for="Telephone" class="control-label">Телефон:</label>
                    <div class="controls"><input type="text" ng-model="u.Telephone"/></div>
                </div>

                <div class="control-group">
                    <label for="URL" class="control-label">URL:</label>
                    <div class="controls"><input type="text" ng-model="u.URL"/></div>
                </div>

                <div class="form-actions">
                    <input type="submit" value="Сохранить"/>

                </div>

            </form>
        </div>
    </div>
</div>
]]></CSP>
</Export>
